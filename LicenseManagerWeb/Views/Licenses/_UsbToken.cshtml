@model UsbTokenLicense

<a asp-controller="Licenses" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary mb-1">Edit Token</a> 

<h4 class="d-flex justify-content-between align-items-center">
        <span class="text-muted">@Model.Name</span>
    </h4>

    <div class="row">
        <div class="col-md-6 mb-4">
            <ul class="list-group">
                <li class="list-group-item d-flex">
                    <div>
                        <label asp-for="K0" class="text-muted"></label>
                        <h6 class="my-0">@Model.K0</h6>
                    </div>
                </li>
                <li class="list-group-item d-flex">
                    <div>
                        <label asp-for="K1" class="text-muted"></label>
                        <h6 class="my-0">@Model.K1</h6>
                    </div>
                </li>
                <li class="list-group-item d-flex">
                    <div>
                        <label asp-for="SerialNumber" class="text-muted"></label>
                        <h6 class="my-0">@Model.SerialNumber</h6>
                    </div>
                </li>
                <partial name="../Licenses/_IssueData" model="Model" />
            </ul>
        </div>
        @if (Model.UsbTokenApi != null)
        {
            <div class="col-md-6 mb-4">
                <ul class="list-group">
                    <li class="list-group-item d-flex">
                        <div>
                            <label asp-for="UsbTokenApi.HwVersion" class="text-muted"></label>
                            <h6 class="my-0">@Model.UsbTokenApi.HwVersion</h6>
                        </div>
                    </li>
                    <li class="list-group-item d-flex">
                        <div>
                            <label asp-for="UsbTokenApi.SwVersion" class="text-muted"></label>
                            <h6 class="my-0">@Model.UsbTokenApi.SwVersion</h6>
                        </div>
                    </li>
                    <li class="list-group-item d-flex">
                        <div>
                            <label asp-for="UsbTokenApi.ApiVersion" class="text-muted"></label>
                            <h6 class="my-0">@Model.UsbTokenApi.ApiVersion</h6>
                        </div>
                    </li>
                </ul>
            </div>
        }
    </div>
