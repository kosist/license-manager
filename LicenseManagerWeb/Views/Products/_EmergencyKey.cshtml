@model EmergencyKey

@if (Model != null)
{
    <h4 class="d-flex justify-content-between align-items-center">
        <span class="text-muted">Emergency Key Information</span>
    </h4>
    <div class="row">
        <div class="col-md-6 mb-4">
            <ul class="list-group">
                <li class="list-group-item d-flex">
                    <div>
                        <label asp-for="Key" class="text-muted"></label>
                        <h6 class="my-0">@Model.Key</h6>
                    </div>
                </li>
                <li class="list-group-item d-flex">
                    <div>
                        <label asp-for="ExecutionIntervalMinutes" class="text-muted"></label>
                        <h6 class="my-0">@Model.ExecutionIntervalMinutes</h6>
                    </div>
                </li>
            </ul>
        </div>
    </div>
}

